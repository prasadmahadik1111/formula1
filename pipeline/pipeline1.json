{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "act_copy_drivers",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_source_api",
						"type": "DatasetReference",
						"parameters": {
							"ds_para_file_name": {
								"value": "@pipeline().parameters.pl_para_drivers",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sink_json",
						"type": "DatasetReference",
						"parameters": {
							"ds_para_directory": {
								"value": "@concat(concat(concat('bronze/',pipeline().parameters.pl_para_drivers),'/'),formatDateTime(utcnow(),'yyyy-MM-dd'))",
								"type": "Expression"
							},
							"ds_para_filepath": {
								"value": "@concat(pipeline().parameters.pl_para_drivers,'.json')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pl_para_drivers": {
				"type": "string",
				"defaultValue": "drivers"
			}
		},
		"annotations": []
	}
}